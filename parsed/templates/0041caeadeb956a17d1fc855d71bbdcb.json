{"components/common/quantity":"{{!\n  Quantity Widget with Increment/Decrement controls.\n\n  @param product\n         cart.item or product object\n\n  @param disabled\n         true or false if the input should be disabled\n}}\n\n<span class=\"form-field-quantity-control\" data-quantity-control=\"{{id}}\">\n  <input\n    class=\"form-input\"\n    type=\"number\"\n    name=\"qty{{#if product_id}}-{{id}}{{else}}[]{{/if}}\"\n    value=\"{{#if quantity}}{{quantity}}{{else}}{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}{{/if}}\"\n    min=\"{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}0{{/if}}\"\n    {{#if max_purchase_quantity}}max=\"{{max_purchase_quantity}}\"{{/if}}\n    data-quantity-control-input\n    {{#if can_purchase '===' false}}disabled{{/if}}\n  >\n\n  <span class=\"quantity-control quantity-decrement {{#if can_purchase '===' false}}quantity-control-blurred{{/if}}\"\n        data-quantity-control-action=\"decrement\">&mdash;</span>\n\n  <span class=\"quantity-control quantity-increment {{#if can_purchase '===' false}}quantity-control-blurred{{/if}}\"\n        data-quantity-control-action=\"increment\">&plus;</span>\n</span>\n","components/common/share-buttons":"{{!-- TODO: add options for switching to Like instead of Share--}}\r\n{{#if share_buttons}}\r\n  <nav class=\"share-buttons-nav\">\r\n    {{#if share_heading}}\r\n      <h2 class=\"share-heading\">{{lang 'common.share_this'}}</h2>\r\n    {{/if}}\r\n\r\n    <ul class=\"share-buttons\" data-share-buttons>\r\n\r\n      {{#each share_buttons.buttons}}\r\n\r\n        {{#if service '===' 'facebook'}}\r\n          <li class=\"social-link facebook\">\r\n            <a target=\"_blank\" href=\"http://www.facebook.com/sharer.php?u={{../../url}}\"\r\n            class=\"facebook\">\r\n              <div class=\"icon-social\">\r\n                <img src=\"https://www.reginascatholicgifts.com/product_images/uploaded_images/facebook.png\">\r\n              </div>\r\n            </a>\r\n          </li>\r\n        {{/if}}\r\n\r\n        {{#if service '===' 'twitter'}}\r\n          <li class=\"social-link twitter\">\r\n            <a target=\"_blank\" href=\"https://twitter.com/share?url={{../../url}}&amp;text={{../../title}}\" class=\"twitter\">\r\n              <div class=\"icon-social\">\r\n                <img src=\"https://www.reginascatholicgifts.com/product_images/uploaded_images/twitter.png\">\r\n              </div>\r\n            </a>\r\n          </li>\r\n        {{/if}}\r\n\r\n        {{#if service '===' 'google_plusone'}}\r\n          <li class=\"social-link google-plus\">\r\n            <a target=\"_blank\" href=\"https://plus.google.com/share?url={{../../url}}\"\r\n            class=\"google-plus\">\r\n              <div class=\"icon-social\">\r\n                <img src=\"https://www.reginascatholicgifts.com/product_images/uploaded_images/gplus.png\">\r\n              </div>\r\n            </a>\r\n          </li>\r\n        {{/if}}\r\n\r\n        {{#if service '===' 'linkedin'}}\r\n          <li class=\"social-link linkedin\">\r\n            <a target=\"_blank\" href=\"http://www.linkedin.com/shareArticle?url={{../../url}}&amp;title={{../../title}}\"\r\n            class=\"linkedin\">\r\n              <div class=\"icon-social\">\r\n                <img src=\"https://www.reginascatholicgifts.com/product_images/uploaded_images/linkedin.png\">\r\n              </div>\r\n            </a>\r\n          </li>\r\n        {{/if}}\r\n        \r\n        {{#if service '===' 'pinterest' }}\r\n          <li class=\"social-link pinterest\">\r\n            <a target=\"_blank\" href=\"//pinterest.com/pin/create/button/?url={{../../url}}&amp;media={{../../image}}&amp;description={{../../title}}\" class=\"share-pinterest\">\r\n              <div class=\"icon-social\">\r\n                <img src=\"https://www.reginascatholicgifts.com/product_images/uploaded_images/pinterest.png\">\r\n              </div>\r\n            </a>\r\n          </li>\r\n        {{/if}}\r\n\r\n        {{#if service '===' 'email'}}\r\n          <li class=\"social-link mail\">\r\n            <a href=\"mailto:?subject={{../../title}}&amp;body={{../../url}}\"\r\n            class=\"email\">\r\n              <svg class=\"icon-social\">\r\n                <use xlink:href=\"#icon-mail\"></use>\r\n              </svg>\r\n            </a>\r\n          </li>\r\n        {{/if}}\r\n\r\n        {{#if service '===' 'print'}}\r\n          <li class=\"social-link print\">\r\n            <a class=\"print\" href=\"javascript:;\" onclick=\"window.print()\">\r\n              <svg class=\"icon-social\">\r\n                <use xlink:href=\"#icon-print\"></use>\r\n              </svg>\r\n            </a>\r\n          </li>\r\n        {{/if}}\r\n\r\n      {{/each}}\r\n\r\n      {{#each share_buttons.buttons}}\r\n        {{#if service '===' 'facebook_like'}}\r\n          <li class=\"social-link facebook-like\">\r\n            <div class=\"fb-like\"\r\n              data-href=\"{{../../url}}\"\r\n              data-layout=\"button_count\"\r\n              data-action=\"like\"\r\n              data-show-faces=\"false\">\r\n            </div>\r\n          </li>\r\n        {{/if}}\r\n      {{/each}}\r\n\r\n    </ul>\r\n  </nav>\r\n{{/if}}\r\n","components/common/shipping-messages":"{{#if shipping_messages}}\n  {{#each shipping_messages}}\n    <div class=\"promo-message\">{{{message}}}</div>\n  {{/each}}\n{{/if}}\n\n{{#if cart.status_messages}}\n  {{#each cart.status_messages}}\n    {{#if type \"==\" 2}}\n      <div class=\"promo-message\">{{{message}}}</div>\n    {{/if}}\n  {{/each}}\n{{/if}}\n\n{{#if product.shipping_messages}}\n  {{#each product.shipping_messages}}\n    <div class=\"promo-message\">{{{message}}}</div>\n  {{/each}}\n{{/if}}\n","components/product/bulk-pricing":"{{#if product.bulk_discount_rates.length}}\n  <div class=\"product-detail-container\">\n    <div class=\"form-field-title\">{{lang 'product.bulk_pricing.title'}}</div>\n    <ul>\n      {{#each product.bulk_discount_rates}}\n        <li>\n          {{lang 'product.bulk_pricing.range' min=min max=max}}\n          {{#if type '===' 'percent'}}\n            {{lang 'product.bulk_pricing.percent' discount=discount.formatted}}\n          {{/if}}\n          {{#if type '===' 'fixed'}}\n            {{lang 'product.bulk_pricing.fixed' discount=discount.formatted}}\n          {{/if}}\n          {{#if type '===' 'price'}}\n            {{lang 'product.bulk_pricing.price' discount=discount.formatted}}\n          {{/if}}\n        </li>\n      {{/each}}\n    </ul>\n  </div>\n{{/if}}\n","components/product/details":"\r\n<div class=\"product-details {{#unless quick_shop}}single-product{{/unless}}\" data-product-details data-product-title=\"{{product.title}}\">\r\n\r\n  {{> components/common/shipping-messages}}\r\n\r\n  <div class=\"product-images\">\r\n    {{> components/product/images\r\n      product_image=product.main_image\r\n      default_image=(cdn theme_settings.default_image)\r\n      product_thumbs=product.images\r\n    }}\r\n  </div>\r\n\r\n  <div class=\"product-info\">\r\n\r\n    <form class=\"form\"\r\n      method=\"post\"\r\n      action=\"{{product.cart_url}}\"\r\n      enctype=\"multipart/form-data\"\r\n      data-product-options-count=\"{{product.options.length}}\"\r\n      data-cart-item-add>\r\n      <input type=\"hidden\" name=\"action\" value=\"add\">\r\n      <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\" data-product-id=\"{{product_id}}\">\r\n\r\n      <header class=\"product-header\">\r\n        <h1 class=\"product-title\">{{product.title}}</h1>\r\n        <a class=\"product-brand\" href=\"{{product.brand.url}}\">{{product.brand.name}}</a>\r\n\r\n        {{> components/product/price price=product.price show_savings=true schema_org=true show_tax_labels=true\r\n        price_as_low_as=theme_settings.price_as_low_as}}\r\n\r\n        {{#if product.call_for_price}}\r\n          <p class=\"product-call-for-price\">{{product.call_for_price}}</p>\r\n        {{/if}}\r\n\r\n        {{{snippet 'product_details'}}}\r\n\r\n        <div>\r\n          {{#if settings.show_product_rating}}\r\n            {{> components/reviews/ratings rating=product.rating}}\r\n            {{{snippet 'product_rating'}}}\r\n          {{/if}}\r\n          {{#if settings.show_product_reviews}}\r\n             <span class=\"ratings-count-text\">\r\n              {{#unless product.num_reviews}}\r\n                {{lang 'product.reviews.no_reviews'}}\r\n              {{/unless}}\r\n              <a href=\"#modal-review-form\" class=\"review-link\" data-review-link>{{ lang 'product.reviews.leave_review' }}</a>\r\n            </span>\r\n          {{/if}}\r\n        </div>\r\n      </header>\r\n\r\n      <div class=\"product-detail-container\">\r\n        {{#if theme_settings.show_product_details}}\r\n          {{#if product.sku}}\r\n            <div class=\"product-detail product-detail-sku\">\r\n              <span class=\"product-detail-key\">{{ lang 'product.detail.sku' }}</span>\r\n              <span data-product-sku>{{product.sku}}</span>\r\n            </div>\r\n          {{/if}}\r\n\r\n          {{#if product.weight}}\r\n            <div class=\"product-detail product-detail-weight\">\r\n              <span class=\"product-detail-key\">{{ lang 'product.detail.weight' }}</span>\r\n              <span data-product-weight>{{product.weight}}</span>\r\n            </div>\r\n          {{/if}}\r\n\r\n          {{#if product.height}}\r\n            <div class=\"product-detail product-detail-height\">\r\n              <span class=\"product-detail-key\">{{ lang 'product.detail.height' }}</span>\r\n              <span data-product-height>{{product.height}}</span>\r\n            </div>\r\n          {{/if}}\r\n\r\n          {{#if product.width}}\r\n            <div class=\"product-detail product-detail-width\">\r\n              <span class=\"product-detail-key\">{{ lang 'product.detail.width' }}</span>\r\n              <span data-product-width>{{product.width}}</span>\r\n            </div>\r\n          {{/if}}\r\n\r\n          {{#if product.depth}}\r\n            <div class=\"product-detail product-detail-depth\">\r\n              <span class=\"product-detail-key\">{{ lang 'product.detail.depth' }}</span>\r\n              <span data-product-depth>{{product.depth}}</span>\r\n            </div>\r\n          {{/if}}\r\n        {{/if}}\r\n\r\n        {{#if product.availability}}\r\n          <div class=\"product-detail product-detail-availability\">\r\n            <span class=\"product-detail-key\">{{ lang 'product.detail.availability' }}</span>\r\n            <span data-product-availability>{{product.availability}}</span>\r\n          </div>\r\n        {{/if}}\r\n\r\n        {{#if theme_settings.show_product_details}}\r\n          {{#if product.shipping.price}}\r\n            <div class=\"product-detail product-detail-shipping-price\">\r\n              <span class=\"product-detail-key\">{{ lang 'product.detail.shipping' }}</span>\r\n              <span data-product-shipping>{{product.shipping.price.formatted}}</span>\r\n            </div>\r\n          {{/if}}\r\n\r\n          {{#if product.min_purchase_quantity}}\r\n            <div class=\"product-detail product-detail-min-purchase-quantity\">\r\n              <span class=\"product-detail-key\">{{ lang 'product.detail.min_purchase' }}</span>\r\n              <span data-product-min-purchase-quantity>{{product.min_purchase_quantity}}</span>\r\n            </div>\r\n          {{/if}}\r\n\r\n          {{#if product.max_purchase_quantity}}\r\n            <div class=\"product-detail product-detail-max-purchase-quantity\">\r\n              <span class=\"product-detail-key\">{{ lang 'product.detail.max_purchase' }}</span>\r\n              <span data-product-max-purchase-quantity>{{product.max_purchase_quantity}}</span>\r\n            </div>\r\n          {{/if}}\r\n\r\n          {{#if product.stock_level}}\r\n            <div class=\"product-detail product-detail-stock-level\">\r\n              <span class=\"product-detail-key\">{{ lang 'product.detail.stock_level' }}</span>\r\n              <span data-product-stock-level>{{product.stock_level}}</span>\r\n            </div>\r\n          {{/if}}\r\n\r\n          {{#if product.gift_wrapping_available}}\r\n            <div class=\"product-detail product-detail-wrapping-available\">\r\n              <span class=\"product-detail-key\">{{ lang 'product.detail.gift_wrapping.title' }}</span>\r\n              <span data-product-gift-wrapping>{{ lang 'product.detail.gift_wrapping.available' }}</span>\r\n            </div>\r\n          {{/if}}\r\n        {{/if}}\r\n\r\n        {{#each product.custom_fields}}\r\n          <div class=\"product-detail product-detail-custom-field\">\r\n            <span class=\"product-detail-key\">{{name}}</span>\r\n            <span data-product-custom-field>{{value}}</span>\r\n          </div>\r\n        {{/each}}\r\n      </div>\r\n\r\n      {{#if product.options}}\r\n        <div data-product-option-change>\r\n          {{#each product.options}}\r\n            {{{dynamicComponent 'core/forms/options'}}}\r\n          {{/each}}\r\n        </div>\r\n      {{/if}}\r\n\r\n      {{#if product.customizations}}\r\n        <div data-product-option-change>\r\n          {{#each product.customizations}}\r\n            {{{dynamicComponent 'core/forms/customizations'}}}\r\n          {{/each}}\r\n        </div>\r\n      {{/if}}\r\n\r\n      {{#if product.release_date}}\r\n        <div class=\"alert alert-info\">\r\n          <div class=\"alert-message\">\r\n            {{product.release_date}}\r\n          </div>\r\n        </div>\r\n      {{/if}}\r\n\r\n      {{#if product.event_date}}\r\n        {{> core/products/event}}\r\n      {{/if}}\r\n\r\n      {{! container for appending cart-add errors}}\r\n\r\n      {{> components/product/bulk-pricing}}\r\n\r\n      <div data-product-cart-message></div>\r\n      <div data-product-option-message></div>\r\n\r\n      <div class=\"form-field form-submit-container {{#if product.can_purchase '===' false}}add-to-cart-blurred{{/if}}\"\r\n           data-product-add>\r\n        {{#if product.show_quantity_input}}\r\n          <label class=\"form-field-quantity-label\">\r\n            <span class=\"form-field-title\">{{lang 'product.quantity'}}</span>\r\n            {{> components/common/quantity product}}\r\n          </label>\r\n        {{/if}}\r\n\r\n        {{#if product.pre_order}}\r\n          <input class=\"button button-primary\" type=\"submit\" value=\"{{lang 'product.pre_order'}}\" data-button-purchase data-button-preorder>\r\n        {{else}}\r\n          <input class=\"button button-primary\"\r\n                 type=\"submit\"\r\n                 value=\"{{lang 'product.add_to_cart'}}\"\r\n                 {{#if product.can_purchase '===' false}}disabled{{/if}}\r\n                 data-button-purchase>\r\n        {{/if}}\r\n        {{{snippet 'product_addtocart'}}}\r\n        <span class=\"spinner\"></span>\r\n      </div>\r\n    </form>\r\n    \r\n    <!-- PayPal Logo -->\r\n    <div class=\"paypal-container\" style=\"padding-bottom: 15px;\">\r\n        <a href=\"https://www.paypal.com/webapps/mpp/paypal-popup\" title=\"How PayPal Works\" onclick=\"javascript:window.open('https://www.paypal.com/webapps/mpp/paypal-popup','WIPaypal','toolbar=no, location=no, directories=no, status=no, menubar=no, scrollbars=yes, resizable=yes, width=1060, height=700'); return false;\"><img src=\"https://www.paypalobjects.com/webstatic/mktg/logo-center/Security_Banner_234x60_2a.gif\" border=\"0\" alt=\"PayPal Logo\"></a>\r\n    </div>\r\n    <!-- PayPal Logo -->\r\n\r\n    {{#unless quick_shop}}\r\n      <div class=\"product-share-container\">\r\n        {{> components/common/share-buttons share_heading=true\r\n          share_buttons=settings.add_this url=product.url title=product.title\r\n          image=(getImage product.main_image 'full')}}\r\n        {{!-- TODO: Hook up wishlist better --}}\r\n        {{#if settings.show_wishlist}}\r\n          <a href=\"{{product.add_to_wishlist_url}}\" class=\"wishlist\">\r\n            <svg class=\"icon-wishlist\">\r\n              <use xlink:href=\"#icon-wishlist\"></use>\r\n            </svg>\r\n            {{lang 'common.wishlist'}}\r\n          </a>\r\n        {{/if}}\r\n      </div>\r\n\r\n      <nav class=\"product-tabs-menu\">\r\n        <ul class=\"product-tabs-list\" data-tabs>\r\n          <li class=\"product-tab-title\"><a class=\"product-tab-link\" href=\"#description\">{{lang 'product.tabs.description'}}</a></li>\r\n          {{#if product.custom_fields.length}}\r\n            {{#if product.detail_messages.length}}\r\n              <li class=\"product-tab-title\"><a class=\"product-tab-link\" href=\"#details\">{{lang 'product.tabs.details'}}</a></li>\r\n            {{/if}}\r\n          {{/if}}\r\n          {{#if product.warranty}}\r\n            <li class=\"product-tab-title\"><a class=\"product-tab-link\" href=\"#warranty\">{{lang 'product.tabs.warranty'}}</a></li>\r\n          {{/if}}\r\n          {{#if product.videos.featured}}\r\n            <li class=\"product-tab-title\"><a class=\"product-tab-link\" href=\"#videos\">{{lang 'product.tabs.videos'}}</a></li>\r\n          {{/if}}\r\n        </ul>\r\n      </nav>\r\n\r\n      <div class=\"product-tabs tabs-content\">\r\n        <div class=\"tab-content-panel rte\" id=\"description\">\r\n          {{#if product.description}}\r\n            <div class=\"tab-product-description\">\r\n              {{{product.description}}}\r\n              {{{snippet 'product_description'}}}\r\n            </div>\r\n          {{/if}}\r\n        </div>\r\n        <div class=\"tab-content-panel\" id=\"details\">\r\n          <div class=\"tab-product-details\">\r\n            {{#if product.detail_messages}}\r\n              {{{product.detail_messages}}}\r\n            {{/if}}\r\n            {{#each product.custom_fields}}\r\n              <dl class=\"product-details-block\">\r\n                <dt class=\"product-details-name\">{{name}}:</dt>\r\n                <dd class=\"product-details-value\">{{value}}</dd>\r\n              </dl>\r\n            {{/each}}\r\n          </div>\r\n        </div>\r\n        {{#if product.warranty}}\r\n          <div class=\"tab-content-panel rte\" id=\"warranty\">\r\n            <div class=\"tab-product-warranty\">\r\n              {{{product.warranty}}}\r\n            </div>\r\n          </div>\r\n        {{/if}}\r\n        {{#if product.videos.featured}}\r\n          <div class=\"tab-content-panel\" id=\"videos\">\r\n            {{#each product.videos.list}}\r\n              <div class=\"tab-product-video-wrapper\">\r\n                <iframe src=\"https://www.youtube.com/embed/{{id}}?rel=0&showinfo=0&autohide=1\" frameborder=\"0\" allowfullscreen></iframe>\r\n              </div>\r\n            {{/each}}\r\n          </div>\r\n        {{/if}}\r\n      </div>\r\n    {{/unless}}\r\n\r\n    {{#if quick_shop}}\r\n      <a class=\"quick-shop-view-product button button-secondary\" href=\"{{product.url}}\">{{lang 'product.view_product'}}</a>\r\n    {{/if}}\r\n  </div>\r\n  {{{snippet 'product_details'}}}\r\n</div>\r\n\r\n{{!--Todo: move this into its own file--}}\r\n{{#unless quick_shop}}\r\n  {{#all product.reviews.list product.num_reviews}}\r\n    <div class=\"product-reviews-container\">\r\n      <div class=\"product-reviews\">\r\n\r\n        {{#each product.reviews.messages}}\r\n          <div class=\"product-review-messages\" data-review-messages>\r\n            {{#if error}}\r\n              {{> core/alerts/alert-error message=error dismissable=true}}\r\n            {{/if}}\r\n            {{#if success}}\r\n              {{> core/alerts/alert-success message=success dismissable=true}}\r\n            {{/if}}\r\n          </div>\r\n        {{/each}}\r\n\r\n        <h3 class=\"product-review-heading section-title\">{{lang 'product.reviews.heading'}}</h3>\r\n\r\n        {{> components/reviews/ratings rating=product.rating}}\r\n\r\n        <header class=\"rating-header\">\r\n          <div class=\"rating-summary\">\r\n            {{{lang 'product.reviews.total_based_on' score=product.rating numReviews=product.num_reviews}}}\r\n          </div>\r\n          <a href=\"#modal-review-form\" class=\"review-link button button-primary button-small\" data-review-link>{{ lang 'product.reviews.leave_review' }}</a>\r\n        </header>\r\n        {{#each product.reviews.list}}\r\n          {{> components/reviews/review-item review_count=../theme_settings.product_review_count}}\r\n        {{/each}}\r\n\r\n        {{#if product.reviews.total \">\" theme_settings.product_review_count}}\r\n          <button class=\"button button-primary button-small view-all-reviews-button\" data-toggle-extra-reviews data-toggle-text=\"{{lang 'product.reviews.view_less'}}\">{{lang 'product.reviews.view_all' count=product.reviews.list.length}}</button>\r\n        {{/if}}\r\n      </div>\r\n    </div>\r\n    {{{snippet 'reviews'}}}\r\n  {{/all}}\r\n\r\n{{/unless}}\r\n","components/product/images":"<div class=\"product-main-image\">\r\n  <img class=\"product-image\" alt=\"{{product_image.alt}}\" src=\"{{getImage product_image 'medium' default_image}}\">\r\n</div>\r\n\r\n{{#if product_thumbs.length \">\" 1}}\r\n  <div class=\"product-thumbnails\">\r\n    {{#each product_thumbs}}\r\n      <span class=\"product-thumbnail {{#if @first}}active{{/if}}\" data-image-thumb data-high-res=\"{{getImage this 'large'}}\">\r\n        <img src=\"{{getImage this 'thumbnail'}}\">\r\n      </span>\r\n    {{/each}}\r\n  </div>\r\n{{/if}}\r\n","components/product/price":"{{!\n  Shows a product's price.\n\n  Prices are fairly complicated. Besides having to be able to show the price\n  with and without tax, a merchant can also assign a Recommended Retail Price,\n  and a Sale Price.\n\n  This means that in extreme cases, we have to show 4 different prices at once.\n\n  When a Sale Price is set we do not have direct access to it--instead it\n  overwrites the regular price.\n\n  @param price object\n\n  @param show_tax_labels bool\n         If true, show a label saying the price is with or without tax.\n\n  @param show_savings bool\n         If true, show a label saying how much is saved from the full price.\n}}\n{{#if price}}\n  {{#or has_options product.options.length}}\n    <div class=\"price {{#all price.without_tax price.with_tax}}multiple-prices{{/all}}\">\n      {{#if price.without_tax}}\n        <span class=\"price-withouttax\" data-product-price-wrapper=\"without-tax\">\n\n          {{#if page_type '!==' 'product'}}\n            {{#if price_as_low_as}}\n              <span>{{lang 'product.as_low_as_price_message'}}</span>\n            {{/if}}\n          {{/if}}\n\n          {{#if price.rrp_without_tax}}\n            <span class=\"price-rrp\" data-price-rrp>\n              {{price.rrp_without_tax.formatted}}\n            </span>\n          {{/if}}\n\n          <span class=\"price-value\" data-product-price>\n            {{price.without_tax.formatted}}\n          </span>\n\n          {{#if show_tax_labels}}\n            {{#if price.with_tax}}\n              <span class=\"price-tax-label\">{{lang 'product.excluding_tax'}}</span>\n            {{/if}}\n          {{/if}}\n\n          {{#all\n            sale_badges\n            price.saved\n          }}\n            <span class=\"sale-badge\">\n              {{lang 'product.sale_badge'}}\n            </span>\n          {{/all}}\n        </span>\n      {{/if}}\n\n      {{#if price.with_tax}}\n        <span class=\"price-withtax\" data-product-price-wrapper=\"with-tax\">\n          {{#if page_type '!==' 'product'}}\n            {{#if price_as_low_as}}\n              <span>{{lang 'product.as_low_as_price_message'}}</span>\n            {{/if}}\n          {{/if}}\n\n          {{#if price.rrp_with_tax}}\n            <span class=\"price-rrp\" data-price-rrp>\n              {{price.rrp_with_tax.formatted}}\n            </span>\n          {{/if}}\n\n          <span class=\"price-value\" data-prodct-price>\n            {{price.with_tax.formatted}}\n          </span>\n\n          {{#if show_tax_labels}}\n            {{#if price.without_tax}}\n              <span class=\"price-tax-label\">{{lang 'product.including_tax'}}</span>\n            {{/if}}\n          {{/if}}\n\n          {{#all sale_badges\n            price.saved}}\n            <span class=\"sale-badge\">\n              {{lang 'product.sale_badge'}}\n            </span>\n          {{/all}}\n        </span>\n      {{/if}}\n\n      {{#if show_savings}}\n        {{#if price.saved}}\n          <span class=\"price-saved\" data-product-price-saved>\n            {{lang 'core.price.you_save' amount=price.saved.formatted}}\n          </span>\n        {{/if}}\n      {{/if}}\n    </div>\n  {{else}}\n    <div class=\"price {{#all price.without_tax price.with_tax}}multiple-prices{{/all}}\">\n\n      {{#if price.without_tax}}\n        <span class=\"price-withouttax\" data-product-price-wrapper=\"without-tax\">\n\n          {{#if price.rrp_without_tax}}\n            <span class=\"price-rrp\" data-price-rrp>\n              {{price.rrp_without_tax.formatted}}\n            </span>\n          {{/if}}\n\n          <span class=\"price-value\" data-product-price>\n            {{price.without_tax.formatted}}\n          </span>\n\n          {{#if show_tax_labels}}\n            {{#if price.with_tax}}\n              <span class=\"price-tax-label\">{{lang 'product.excluding_tax'}}</span>\n            {{/if}}\n          {{/if}}\n\n          {{#all\n            sale_badges\n            price.saved\n          }}\n            <span class=\"sale-badge\">\n              {{lang 'product.sale_badge'}}\n            </span>\n          {{/all}}\n        </span>\n      {{/if}}\n\n      {{#if price.with_tax}}\n        <span class=\"price-withtax\" data-product-price-wrapper=\"with-tax\">\n\n          {{#if price.rrp_with_tax}}\n            <span class=\"price-rrp\" data-price-rrp>\n              {{price.rrp_with_tax.formatted}}\n            </span>\n          {{/if}}\n\n          <span class=\"price-value\" data-prodct-price>\n            {{price.with_tax.formatted}}\n          </span>\n\n          {{#if show_tax_labels}}\n            {{#if price.without_tax}}\n              <span class=\"price-tax-label\">{{lang 'product.including_tax'}}</span>\n            {{/if}}\n          {{/if}}\n\n          {{#all\n            sale_badges\n            price.saved\n          }}\n            <span class=\"sale-badge\">\n              {{lang 'product.sale_badge'}}\n            </span>\n          {{/all}}\n        </span>\n      {{/if}}\n\n      {{#if show_savings}}\n        {{#if price.saved}}\n          <span class=\"price-saved\" data-product-price-saved>\n            {{lang 'core.price.you_save' amount=price.saved.formatted}}\n          </span>\n        {{/if}}\n      {{/if}}\n    </div>\n  {{/or}}\n{{/if}}\n","components/reviews/ratings":"<div class=\"rating-stars\" aria-hidden=\"true\">\n  {{#for 1 5}}\n    {{#if ../this.rating '>=' $index}}\n      <svg class=\"rating-star star-full\">\n        <use xlink:href=\"#icon-star\"></use>\n      </svg>\n    {{else}}\n      <svg class=\"rating-star star-empty\">\n        <use xlink:href=\"#icon-star-empty\"></use>\n      </svg>\n    {{/if}}\n  {{/for}}\n</div>\n","components/reviews/review-item":"\n<article class=\"review-item {{#if @index '>=' review_count}}supplementary{{else}} default{{/if}}\">\n  <header class=\"review-item-header\">\n    <div class=\"review-item-stars\">\n      {{> components/reviews/ratings rating=rating size=\"medium\"}}\n      <span>{{rating}}/5</span>\n    </div>\n    <h5 class=\"review-item-title\">{{title}}</h5>\n    <div class=\"review-item-meta\">\n      {{lang 'product.reviews.written_by_on' name=name date=date}}\n    </div>\n  </header>\n  <p class=\"review-item-body\">{{text}}</p>\n</article>\n","core/alerts/alert-error":"<div class=\"alert alert-error\">\n  {{#if dismissable}}<a class=\"alert-dismiss\">&times;</a>{{/if}}\n\n  <div class=\"alert-message\">\n    {{#if message}}\n      {{{message}}}\n    {{else}}\n      {{{this}}}\n    {{/if}}\n  </div>\n</div>\n","core/alerts/alert-success":"<div class=\"alert alert-success\">\n  {{#if dismissable}}<a class=\"alert-dismiss\">&times;</a>{{/if}}\n\n  <div class=\"alert-message\">\n    {{#if message}}\n      {{{message}}}\n    {{else}}\n      {{{this}}}\n    {{/if}}\n  </div>\n</div>\n","core/forms/customizations/checkbox":"\n<div class=\"form-field form-field-customizations form-field-checkbox{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-control\">\n      <input class=\"form-input form-checkbox\"\n        id=\"product-field-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"checkbox\"\n        {{#if value}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n      <span class=\"form-label-text product-customization-text\">\n        {{display_name}}\n        {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n      </span>\n    </span>\n  </label>\n</div>\n","core/forms/customizations/file":"\n<div class=\"form-field form-field-customizations form-field-file {{#if required}}form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'core.account.products.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">{{lang 'core.account.products.filetypes' file_types=file_types}}</span>\n  {{/if}}\n\n  <span class=\"form-file-size\">{{lang 'core.account.products.max_filesize' bytes=file_size}}</span>\n</div>\n","core/forms/customizations/select":"\n<div class=\"form-field form-field-customizations form-field-select{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper\">\n        <select\n          class=\"form-input input-select\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          <option value=\"\" {{#any values selected=true}}{{else}}selected{{/any}} disabled>{{lang 'product.pick_one'}}</option>\n          {{#each values}}\n            <option value=\"{{this}}\" {{#if selected}}selected{{/if}}>{{this}}</option>\n          {{/each}}\n        </select>\n      </span>\n    </span>\n  </label>\n</div>\n","core/forms/customizations/text":"\n<div class=\"form-field form-field-customizations form-field-text{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\" for=\"product-fields-{{id}}\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        value=\"{{value}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/customizations/textarea":"\n<div class=\"form-field form-field-customizations form-field-textarea{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}:\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        rows=\"4\"\n        placeholder=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"></textarea>\n    </span>\n  </label>\n</div>\n","core/forms/options/date":"\n<div class=\"form-field form-field-options form-field-date{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"date\">\n  <span class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][year]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.year'}}</option>\n          {{#if earliest_year '===' latest_year}}\n            <option value=\"{{$index}}\" selected=\"selected\">{{$index}}</option>\n          {{else}}\n            {{#for earliest_year latest_year}}\n              <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>{{$index}}</option>\n            {{/for}}\n          {{/if}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][month]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.month'}}</option>\n          {{#for 1 12}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>{{getShortMonth $index}}</option>\n          {{/for}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][day]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.day'}}</option>\n          {{#for 1 31}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>{{$index}}</option>\n          {{/for}}\n        </select>\n        </span>\n    </span>\n\n  </span>\n</div>\n","core/forms/options/input-checkbox":"\n<div class=\"form-field form-field-options form-field-checkbox{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-checkbox\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    <label class=\"form-label {{class}}\">\n      <input\n        class=\"form-input form-checkbox\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"checkbox\"\n        value=\"{{value}}\"\n        {{#if checked}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n      <span class=\"form-label-text\">{{label}}</span>\n    </label>\n  </div>\n</div>\n","core/forms/options/input-file":"\n<div class=\"form-field form-field-options form-field-file {{#if required}}form-required{{/if}}\"\n  data-product-attribute=\"input-file\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'core.account.products.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">{{lang 'core.account.products.filetypes' file_types=file_types}}</span>\n  {{/if}}\n\n  <span class=\"form-file-size\">{{lang 'core.account.products.max_filesize' bytes=file_size}}</span>\n</div>\n","core/forms/options/input-numbers":"\n<div class=\"form-field form-field-options form-field-number{{#if required}} form-required{{/if}}\"\n data-product-attribute=\"input-number\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\" id=\"attribute-{{id}}\" name=\"attribute[{{id}}]\" type=\"number\" value=\"{{prefill}}\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/options/input-text":"\n<div class=\"form-field form-field-options form-field-text{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-text\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        value=\"\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/options/product-list":"\n<div class=\"form-field form-field-options form-field-product-list{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"product-list\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    {{#unless values.0.image}}\n      {{!-- same as radio buttons if no image set --}}\n      {{#each values}}\n        <label class=\"form-label\" data-product-attribute-value=\"{{id}}\">\n          <input\n            class=\"form-input form-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\"\n            value=\"{{id}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"form-label-text\">{{label}}</span>\n        </label>\n      {{/each}}\n      {{#unless required}}\n        <label class=\"form-label\">\n          <input\n            class=\"form-input form-radio\"\n            name=\"attribute[{{id}}]\"\n            type=\"radio\"\n            value=\"0\"\n            {{#if prefill '==' 0}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"form-label-text\">{{lang 'product.none'}}</span>\n        </label>\n      {{/unless}}\n    {{else}}\n      {{#each values}}\n        <label class=\"form-label product-picklist-item\" data-product-attribute-value=\"{{id}}\">\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\" value=\"{{id}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"product-picklist-item-body\">\n            <span class=\"product-picklist-image-wrap\">\n              <img class=\"product-picklist-image\" src=\"{{getImage image 'core-product-picklist' (cdn ../theme_settings.default_image)}}\" alt=\"{{image.alt}}\">\n            </span>\n            <span class=\"product-picklist-label-text form-label-text\">{{label}}</span>\n          </span>\n        </label>\n      {{/each}}\n      {{#unless required}}\n        <label class=\"form-label product-picklist-item\">\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{id}}]\"\n            type=\"radio\"\n            value=\"0\"\n            {{#if prefill '==' 0}}checked{{/if}}>\n          <span class=\"product-picklist-item-body picklist-none\">\n            <span class=\"product-picklist-label-text form-label-text\">{{lang 'product.none'}}</span>\n          </span>\n        </label>\n      {{/unless}}\n    {{/unless}}\n  </div>\n</div>\n","core/forms/options/set-radio":"\n<div class=\"form-field form-field-options form-field-radio{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-radio\">\n  <div class=\"form-field-title product-option-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-radio\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n  </div>\n</div>\n","core/forms/options/set-rectangle":"\n<div class=\"form-field form-field-options form-field-rectangle{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-rectangle\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label rectangle\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-rectangle\"\n          name=\"attribute[{{../id}}]\"\n          id=\"attribute-{{id}}\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"rectangle-text form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n  </div>\n</div>\n","core/forms/options/set-select":"\n<div class=\"form-field form-field-options form-field-select{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-select\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <div class=\"form-select-wrapper\">\n        <select\n          class=\"form-input form-select\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          <option value=\"\" {{#any values selected=true}}{{else}}selected{{/any}} disabled>{{lang 'product.pick_one'}}</option>\n          {{#each values}}\n            <option value=\"{{id}}\" {{#if selected}}selected{{/if}} data-product-attribute-value=\"{{id}}\">{{label}}</option>\n          {{/each}}\n        </select>\n      </div>\n    </span>\n  </label>\n</div>\n","core/forms/options/swatch":"\n<div class=\"form-field form-field-options form-field-swatch{{#if required}} form-required{{/if}}\"\n  data-swatch-selector\n  data-product-attribute=\"swatch\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    <span class=\"swatch-value\" data-swatch-value>{{lang 'forms.swatches_none_selected'}}</span>\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"swatch-wrap\" data-swatch-value=\"{{label}}\" data-product-attribute-value=\"{{id}}\">\n        <span class=\"form-label-text\">{{label}}</span>\n\n        <input\n          class=\"form-input swatch-radio\"\n          id=\"attribute-{{id}}\"\n          type=\"radio\"\n          name=\"attribute[{{../id}}]\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n\n        <span class=\"swatch {{#if data.[1]}}two-colors{{/if}}{{#if data.[2]}} three-colors{{/if}}\">\n          {{#if pattern}}\n            <span class=\"swatch-color swatch-pattern\" style=\"background-image: url('{{getImage image 'core-swatch'}}');\"></span>\n          {{/if}}\n\n          {{#if data.[0]}}\n            <span class=\"swatch-color\" style=\"background-color: #{{data.[0]}}\"></span>\n          {{/if}}\n\n          {{#if data.[1]}}\n            <span class=\"swatch-color secondary\" style=\"background-color: #{{data.[1]}}\"></span>\n          {{/if}}\n\n          {{#if data.[2]}}\n            <span class=\"swatch-color tertiary\" style=\"background-color: #{{data.[2]}}\"></span>\n          {{/if}}\n        </span>\n\n        {{#if pattern}}\n          <span class=\"swatch-pattern-expanded\">\n            <img class=\"swatch-pattern-image\" src=\"{{getImage image 'core-swatch'}}\">\n          </span>\n        {{/if}}\n\n      </label>\n    {{/each}}\n  </div>\n\n</div>\n","core/forms/options/textarea":"\n<div class=\"form-field form-field-options form-field-textarea{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"textarea\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        rows=\"4\"\n        placeholder=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"></textarea>\n    </span>\n  </label>\n</div>\n","core/products/event":"<div class=\"form-field form-field-date form-required\">\n  <span class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{product.event_date.name}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"EventDate[Yr]\" required>\n          <option disabled selected>{{lang 'forms.date.year'}}</option>\n          {{#if product.event_date.earliest_year '==' product.event_date.latest_year}}\n            <option value=\"{{product.event_date.earliest_year}}\">{{product.event_date.earliest_year}}</option>\n          {{else}}\n            {{#for product.event_date.earliest_year product.event_date.latest_year}}\n              <option value=\"{{$index}}\">{{$index}}</option>\n            {{/for}}\n          {{/if}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select class=\"form-input form-select\" name=\"EventDate[Mth]\" required>\n          <option disabled>{{lang 'forms.date.month'}}</option>\n          {{#for 1 12}}\n            <option value=\"{{$index}}\">{{getShortMonth $index}}</option>\n          {{/for}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select class=\"form-input form-select\" name=\"EventDate[Day]\" required>\n          <option disabled>{{lang 'forms.date.day'}}</option>\n          {{#for 1 31}}\n            <option value=\"{{$index}}\">{{$index}}</option>\n          {{/for}}\n        </select>\n      </span>\n    </span>\n\n  </span>\n\n  {{#if product.event_date.date_start}}\n    {{#if product.event_date.date_end}}\n      <span class=\"product-event-bounds\">{{lang 'forms.date.bounds' start_date=product.event_date.date_start end_date=product.event_date.date_end}}</span>\n    {{/if}}\n  {{/if}}\n</div>\n"}
